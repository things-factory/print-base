{
  "name": "@things-factory/print-base",
  "version": "0.1.16",
  "main": "dist-server/index.js",
  "browser": "client/index.js",
  "things-factory": true,
  "license": "MIT",
  "author": "",
  "scripts": {
    "serve": "things-factory",
    "serve:dev": "npm run build:server && things-factory-dev",
    "build": "npm run build:client",
